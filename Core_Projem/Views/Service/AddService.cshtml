@model EntityLayer.Concrate.Service
@{
    ViewData["Title"] = "AddService";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}

<div style="margin-left:35%;margin-top:8%;">
    <br /><br /><br />
    <h4 style="margin-left:10px;">Hizmet Ekleme Sayfası</h4>
    <form method="post" class="col-xl-5 col-md-5 table-bordered" style="height:270px;">
        <br />
        <b>@Html.Label("Başlık")</b>
        @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
        <br />
        <b>@Html.Label("Resim Url")</b>
        @Html.TextBoxFor(x => x.ImageUrl, new { @class = "form-control" })
        <br />
        <button class="btn btn-success">Kaydet</button>
    </form>
</div>

